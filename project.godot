config_version=5

[application]

config/name="multiplayerFPStest"
run/main_scene="res://assets/scenes/world.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")

[audio]

buses/default_bus_layout="res://assets/sounds/Master.tres"

[display]

window/size/viewport_width=480
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=960
window/stretch/mode="viewport"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[file_customization]

folder_colors={
"res://assets/": "orange",
"res://assets/scenes/": "yellow",
"res://assets/scenes/scripts/": "blue",
"res://assets/sounds/": "green"
}

